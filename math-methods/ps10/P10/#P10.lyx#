\begin_layout Plain Layout

% Part (a)
\end_layout

\begin_layout Plain Layout

n=5000;
\end_layout

\begin_layout Plain Layout

a=1.4;
\end_layout

\begin_layout Plain Layout

b=0.3;
\end_layout

\begin_layout Plain Layout

x=zeros(n,1);
\end_layout

\begin_layout Plain Layout

y=zeros(n,1);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

for i=2:n
\end_layout

\begin_layout Plain Layout

   x(i) = a - (x(i-1))^2 + b*y(i-1);
\end_layout

\begin_layout Plain Layout

   y(i) = x(i-1);
\end_layout

\begin_layout Plain Layout

end
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

plot(1:5000,x,'Color',[0.5,0.5,0.5])
\end_layout

\begin_layout Plain Layout

print('Fig_a1','-dpng')
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

plot(x,y,'Color',[0.5,0.5,0.5])
\end_layout

\begin_layout Plain Layout

print('Fig_a2','-dpng')
\end_layout

\end_inset


\end_layout

