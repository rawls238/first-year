inline false
status open

\begin_layout Plain Layout

% Part (c)
\end_layout

\begin_layout Plain Layout

xx=zeros(n,1);
\end_layout

\begin_layout Plain Layout

yy=zeros(n,1);
\end_layout

\begin_layout Plain Layout

xx(1)=10^(-8);
\end_layout

\begin_layout Plain Layout

yy(1)=10^(-8);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

for i=2:n
\end_layout

\begin_layout Plain Layout

   xx(i) = a - (xx(i-1))^2 + b*yy(i-1);
\end_layout

\begin_layout Plain Layout

   yy(i) = xx(i-1);
\end_layout

\begin_layout Plain Layout

end
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

plot(1:5000,xx,'Color',[0.5,0.5,0.5])
\end_layout

\begin_layout Plain Layout

print('Fig_c1','-dpng')
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

plot(xx,yy,'Color',[0.5,0.5,0.5])
\end_layout

\begin_layout Plain Layout

print('Fig_c2','-dpng')
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

histogram(xx,40,'FaceColor',[0.5,0.5,0.5])
\end_layout

\begin_layout Plain Layout

print('Fig_c3','-dpng')
\end_layout

